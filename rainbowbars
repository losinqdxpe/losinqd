	CE:FireEvent("PompousTheCloud", MainUserW["Bar1"]["Handle"],"BrickColor",BrickColor.new("Dark blue"))
	CE:FireEvent("PompousTheCloud", MainUserW["Bar2"]["Handle"],"BrickColor",BrickColor.new("Dark blue"))
	CE:FireEvent("PompousTheCloud", MainUserW["Bar3"]["Handle"],"BrickColor",BrickColor.new("Dark blue"))
	CE:FireEvent("PompousTheCloud", MainUserW["Bar4"]["Handle"],"BrickColor",BrickColor.new("Dark blue"))
