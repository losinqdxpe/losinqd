		CE:FireEvent("PompousTheCloud", MainUserW["Bar4"]["Handle"],"Color",Color3.new(255/255,0/255,0/255))
	for i = 0,255,10 do
		CE:FireEvent("PompousTheCloud", MainUserW["Bar1"]["Handle"],"Color",Color3.new(255/255,i/255,0/255))
		CE:FireEvent("PompousTheCloud", MainUserW["Bar2"]["Handle"],"Color",Color3.new(255/255,i/255,0/255))
		CE:FireEvent("PompousTheCloud", MainUserW["Bar3"]["Handle"],"Color",Color3.new(255/255,i/255,0/255))
		CE:FireEvent("PompousTheCloud", MainUserW["Bar4"]["Handle"],"Color",Color3.new(255/255,i/255,0/255)) 
	end
	for i = 255,0,-10 do
		CE:FireEvent("PompousTheCloud", MainUserW["Bar1"]["Handle"],"Color",Color3.new(i/255,255/255,0/255))
